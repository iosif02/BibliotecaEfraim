{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["es2017", "DOM", "dom.iterable", "esnext"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
  },
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
